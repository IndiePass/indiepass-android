    <string name="pref_key_sensitivity_title">Post sensitivity</string>
    <string name="pref_key_sensitivity_summary">Allow marking a post as sensitive.</string>
    <string name="pref_key_location_visibility_title">Location visibility</string>
    <string name="pref_key_location_visibility_summary">When location is found, show a select widget that allows you to send the location visibility. Only available for IndieWeb accounts.</string>
    <string name="pref_key_location_label_title">Location name</string>
    <string name="pref_key_location_label_summary">Expose a field to enter the location name on note, article and event. This field is always exposed on the checkin and geocache post type.</string>
    <string name="pref_key_location_label_query_title">Location name query</string>
    <string name="pref_key_location_label_query_summary">Allow querying for a location name based on the found coordinates. Leave disabled if your endpoint does not support the ?q=geo query.</string>
    <string name="pref_key_tags_list_title">Category list</string>
    <string name="pref_key_tags_list_summary">Get a list of categories from your endpoint for autocompletion. They will be saved, so they can be used offline as well. Leave disabled if your endpoint does not support the ?q=category query. Only available for IndieWeb accounts.</string>
    <string name="pref_key_post_type_hide_title">Hide post types</string>
    <string name="pref_key_post_type_hide_summary">Hide post types in the menu which are not supported or enabled by your Micropub endpoint. Leave disabled if your endpoint does not send post-types in the ?q=config query. Only available for IndieWeb accounts.</string>
    <string name="pref_key_access_token_body_title">Send access token in body</string>
    <string name="pref_key_access_token_body_summary">The access token for a Micropub request is sent along in the header by default. If enabled, this setting will send it in the body of the request instead of the header.</string>
    <!-- Preferences: Reader -->
    <string name="pref_category_reader_title">Reader</string>
    <string name="pref_category_reader_summary">Control reader settings</string>
    <string name="pref_key_reader_cache_title">Offline reading</string>
    <string name="pref_key_reader_cache_summary">Stores the list of channels and items offline, so you can read without connection and a cache item is available. A \"Clear cache\" action will be available in the menu of the channel list.</string>
    <string name="pref_key_video_autoplay_title">Video autoplay</string>
    <string name="pref_key_timeline_summary_detail_click_title">Summary detail click</string>
    <string name="pref_key_timeline_summary_detail_click_summary">Clicking on an item in the summary style opens the full detail view of the post. In compact mode this is always the case.</string>
    <string name="pref_key_image_preview_title">Image preview on summary</string>
    <string name="pref_key_image_preview_summary">Show the first image per post in the summary style. Click on the image information to see all images.</string>
    <string name="pref_key_image_preview_compact_title">Image preview on compact</string>
    <string name="pref_key_image_preview_compact_summary">Show the first image per post in the compact style. The images are always visible on the full detail of the post.</string>
    <string name="pref_key_mark_read_title">Mark items read</string>
    <string name="pref_key_mark_read_summary">Configure how items are marked as read.\n\n1: mark all items read when viewing a channel, which is per page.\n2: manually with a \"Mark all read\" button per channel (only shows up in case there are more than 20 items) and \"Mark read\" action in the item menu.\n3: mark read while scrolling.\n\nOnly available for IndieWeb accounts.</string>
    <string name="pref_key_search_title">Channel search</string>
    <string name="pref_key_search_summary">Allows to search in a channel for posts</string>
    <string name="pref_key_search_global_title">Global search</string>
    <string name="pref_key_search_global_summary">Allows to search for posts across all channels</string>
    <string name="pref_key_author_timeline_title">Timeline per author</string>
    <string name="pref_key_author_timeline_summary">Makes the author avatar clickable to see all posts by this author if your server supports this feature</string>
    <string name="pref_key_unread_items_channel_title">Unread items channel</string>
    <string name="pref_key_unread_items_channel_summary">Adds a channel at the top of the list which allows you to view all unread items across all channels at once. Will not show up if there are no unread items or only within one channel. Marking items as read needs to happen manually. Only available for IndieWeb accounts.</string>
    <string name="pref_key_timeline_author_original_title">Show original author</string>
    <string name="pref_key_timeline_author_original_summary">Swaps the name and photo by the original author for reposts, quotations and likes, if available. Only available for IndieWeb accounts.</string>
    <string name="pref_key_timeline_recursive_title">Check references recursively</string>
    <string name="pref_key_timeline_recursive_summary">Posts can have references to other posts. And those posts can have references again. Toggle this setting to check a level deeper for getting extra content if available. This is currently only enabled for reposts.</string>
    <string name="pref_key_timeline_autoload_more_title">Autoload more posts</string>
    <string name="pref_key_timeline_autoload_more_summary">When the \'Load more\' button comes into view, autoload more posts instead of having to click manually.</string>
    <string name="pref_key_move_item_title">Move items</string>
    <string name="pref_key_move_item_summary">Allows moving posts from one channel to another. Only available for IndieWeb accounts.</string>
    <string name="pref_key_read_later_title">Read later channel</string>
    <string name="pref_key_read_later_summary">Enter the id of the channel you use as a \'Read later\' channel. Marking items as read needs to happen manually. Only available for IndieWeb accounts.</string>
    <string name="pref_key_response_bookmark_title">Enable bookmark response</string>
    <string name="pref_key_response_read_title">Enable read response</string>
    <!-- Preferences: Media handling -->
    <string name="pref_category_media_title">Media</string>
    <string name="pref_category_media_summary">Control image and video settings for uploading</string>
    <string name="pref_key_image_scale_title">Scale images on upload</string>
    <string name="pref_key_image_scale_summary">Scale images when uploading. This results in smaller and faster uploads, but removes Exif data.</string>
    <string name="pref_key_image_quality_title">Image upload quality</string>
    <string name="pref_key_image_quality_summary">Define the quality for image manipulations. Ranges from 0 to 100. Higher values mean better image quality but bigger files.</string>
    <string name="pref_key_image_size_title">Image upload size</string>
    <string name="pref_key_image_size_summary">Maximum image size in for width or height. In case the preview is blank, set the default size lower as this indicates the phone has run out of memory.</string>
    <string name="pref_key_media_name_title">Add name to upload</string>
    <string name="pref_key_media_name_summary">Exposes a textfield on the Upload screen, so you can send a title for your uploaded file to the media endpoint. Only available for IndieWeb accounts.</string>
    <string name="pref_key_upload_media_endpoint_title">Use media endpoint to upload</string>
    <string name="pref_key_upload_media_endpoint_summary">Use the media endpoint, if available, when attaching media to a post.</string>
    <!-- Preferences: debugging -->
    <string name="pref_category_debug_title">Developer</string>
    <string name="pref_category_debug_summary">Add helper functionality to help debugging Indigenous or your application.</string>
    <string name="pref_key_debug_microsub_channels_json_title">Channels JSON</string>
    <string name="pref_key_debug_microsub_channels_json_summary">Adds a \"Debug\" option to the channels list in the reader to view the JSON representation.</string>
    <string name="pref_key_debug_microsub_timeline_json_title">Timeline JSON</string>
    <string name="pref_key_debug_microsub_timeline_json_summary">Adds a \"Debug\" option to every timeline in the reader to view the JSON representation.</string>
    <string name="pref_key_debug_microsub_item_json_title">Reader item JSON</string>
    <string name="pref_key_debug_microsub_item_json_summary">Adds a \"Debug\" option to every item in the reader to view the JSON representation.</string>
    <string name="pref_key_debug_source_list_title">Posts lists JSON</string>
    <string name="pref_key_debug_source_list_summary">Adds a \"Debug\" option to the post lists to view the JSON representation.</string>
    <string name="pref_key_debug_contact_list_title">Contacts lists JSON</string>
    <string name="pref_key_debug_contact_list_summary">Adds a \"Debug\" option to the contact lists to view the JSON representation.</string>
    <!-- Preferences: contacts -->
    <string name="pref_category_contact_title">Contacts</string>
    <string name="pref_category_contact_summary">Leave disabled if your endpoint does not support the ?q=contact query. Only available for IndieWeb accounts.</string>
    <string name="pref_key_contact_body_autocomplete_title">Contact body autocomplete</string>
    <string name="pref_key_contact_body_autocomplete_summary">Get a list of contacts from your endpoint for autocompletion in body fields when you start typing an @. They will be saved, so they can be used offline as well.</string>
    <string name="pref_key_contact_body_autocomplete_value_title">Use nickname for autocomplete</string>
    <string name="pref_key_contact_body_autocomplete_value_summary">Contacts can have name and nickname. If a nickname is available, that value will be used for the suggestions instead of the full name.</string>
    <string name="pref_key_contact_manage_title">Manage contacts</string>
    <string name="pref_key_contact_manage_summary">This will add a menu item that allows you to manage your contacts. An action will be added to every timeline item too to save the author of a post to your contacts.</string>
    <!-- Preferences: experimental -->
    <string name="pref_category_experimental_title">Experimental</string>
    <string name="pref_category_experimental_summary">Experimental features which are either not fully mature yet from a spec perspective or have little support by servers. Only available for IndieWeb accounts.</string>
    <string name="pref_key_source_post_list_title">Posts list</string>
    <string name="pref_key_source_post_list_summary">Expose a \"Posts list\" which lists the content of your site. Leave disabled if your micropub endpoint does not support the ?q=source query.</string>
    <string name="pref_key_source_update_title">Update posts</string>
    <string name="pref_key_source_update_summary">Expose an \"Update\" screen which allows you to update basic properties of a post.</string>
    <string name="pref_key_source_delete_title">Delete posts</string>
    <string name="pref_key_source_delete_summary">Expose a delete button on the update screen, or in the posts list.</string>
